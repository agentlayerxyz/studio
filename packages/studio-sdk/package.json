{
  "name": "@agentstudio/sdk",
  "version": "0.0.1",
  "description": "AgentStudio SDK is a set of tools and services maintained by [AgentLayer](https://agentlayer.xyz) as an effort to make it easier to build, test, and deploy AI agents. ",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentlayer/agentstudio.git",
    "directory": "packages/studio-sdk"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "agentlayer",
    "agentstudio",
    "sdk",
    "ai",
    "blockchain"
  ],
  "author": "AgentLayer",
  "license": "MIT",
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "changeset": "changeset"
  },
  "dependencies": {
    "@agentlayer/eslint-config": "workspace:*",
    "@agentlayer/logging": "workspace:*",
    "@agentlayer/typescript-config": "workspace:*",
    "openapi-fetch": "^0.13.5",
    "viem": "^2.23.14",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "openapi-typescript": "^7.6.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.4"
  }
}
